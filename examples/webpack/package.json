    "source-map-loader": "^0.2.3",
    "ts-loader": "^9.2.6",
    "telefunc": "0.1.0-beta.1",
    "typescript": "^4.4.3",
    "url-loader": "^1.0.1",
    "webpack": "^5.56.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.11"
